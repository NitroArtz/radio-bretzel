#Icecast Server Parametters.
#These parametters apply for every source aka every liquidsoap container.

# host.domain of the icecast server. Here, using "bretzel" domain (app internal domain)
host = "icecast.bretzel"
# icecast server port
port = 8000
# source password. Set in radio-bretzel/icecast/icecast.xml
src_passwd = "sourcepass"
